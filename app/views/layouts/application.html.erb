<!DOCTYPE html>
<html>
<head>

	<%= render 'layouts/header' %>
  <title>Pinterest Clone</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>


<body>
	<div class="container">
		<% flash.each do |type, message| %>
